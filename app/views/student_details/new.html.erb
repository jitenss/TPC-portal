<div>
	<%= errors(@student)%>
		<%= form_for(:student_form, :url=>{:action=>'create'}) do |f| %>
			<table>
				<%= render(:partial=>'formStudent',:locals=>{:f=>f}) %>
				<tr>
					<th><%=f.label(:password)%></th>
					<td><%=f.password_field(:password)%></td>
				</tr>

				<tr>
					<th><%=f.label(:password_confirmation)%></th>
					<td><%=f.password_field(:password_confirmation)%></td>
				</tr>
			</table>

			<%= submit_tag("Register") %>
		<%end%>
	
</div>