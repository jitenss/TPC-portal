<table summary="Subject form fields">
  <tr>
    <th> <%=f.label(:cin,'CIN')%></th>
    <td><%= f.text_field(:cin) %></td>
  </tr>
  <tr>
    <th><%= f.label(:c_name)%></th>
    <td><%= f.text_field(:c_name) %></td>
  </tr>
  <tr>
    <th><%= f.label(:cpiCriteria)%></th>
    <td><%= f.text_field(:cpiCriteria) %></td>
  </tr>
  <tr>
    <th><%= f.label(:category)%></th>
    <td><%= f.select(:category,['Regular','Dream','Core','Super Dream']) %></td>
  </tr>
  <tr>
    <th><%= f.label(:batch)%></th>
    <td><%= f.text_field(:batch) %></td>
  </tr>
  <tr>
    <th><%= f.label(:backlogs)%></th>
    <td><%= f.text_field(:backlogs) %></td>
  </tr>
  <tr>
    <th><%= f.label(:skills_set)%></th>
    <td><%= f.text_area(:skills_set,:size=>'30x10') %></td>
  </tr>
  <tr>
    <th><%= f.label(:info)%></th>
    <td><%= f.text_area(:info,:size=>'40x10') %></td>
  </tr>
</table>
