<%= link_to("<< Back to List", {:action => 'index'}) %>


  <h2>Show Company</h2>
  <table>
    <tr>
      <th>Cin</th>
      <td><%= @company.cin%></td>
    </tr>
    <tr>
      <th>Name</th>
      <td><%= @company.c_name %></td>
    </tr>
    <tr>
      <th>Category</th>
      <td><%= @company.category %></td>
    </tr>
    <tr>
      <th>CPI Criteria</th>
      <td><%= @company.cpiCriteria %></td>
    </tr>
    <tr>
      <th>Backlogs</th>
      <td><%= @company.c_backlogs %></td>
    </tr>
    <tr>
      <th>Skills Set</th>
      <td><%= @company.skills_set %></td>
    </tr>
    <tr>
      <th>Company Info</th>
      <td><%= @company.info %></td>
    </tr>
    <tr>
      <th>Created</th>
      <td><%= @company.created_at %></td>
    </tr>
    <tr>
      <th>Updated</th>
      <td><%= @company.updated_at %></td>
    </tr>
    <tr>
      <th>Visible</th>
      <td><%= @company.visiting ? 'true' : 'false' %></td>
    </tr>
    <tr>
      <th>Branch</th>
      <% @company.company_branches.each do|c|%>s
      <td><%= c.branch %></td>
      <% end %>
